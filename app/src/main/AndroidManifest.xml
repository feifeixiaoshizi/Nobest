<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.study.nobest">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".DefaultApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".TestActivity"/>
        <activity android:name=".TestHandlerActivity"/>
        <activity android:name=".rxjava.TestRxjava2Activity"/>
        <activity android:name=".rxjava.TestRxjava2DisposeActivity"/>
        <activity android:name=".recyclerview.test.TestRecyclerViewActivity"/>
        <activity android:name=".recyclerview.test.TestRecyclerViewActivity1"/>
        <activity android:name=".recyclerview.test.TestRecyclerViewGrideActivity"/>
        <activity android:name=".recyclerview.test.TestRecyclerViewLayoutManagerActivity"/>
        <activity android:name=".recyclerview.test.TestRecyclerViewSnapHelperActivity"/>
        <activity android:name=".recyclerview.test.TestRecyclerViewTochHelperActivity"/>
    </application>

</manifest>